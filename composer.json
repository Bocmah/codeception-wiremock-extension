{
    "keywords": [
        "wiremock",
        "extension",
        "module",
        "codeception",
        "mock",
        "mock external services"
    ],
    "authors": [
        {
            "name": "Mariano Custiel",
            "email": "jmcustiel@gmail.com",
            "homepage": "https://github.com/mcustiel",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Codeception\\": "src"
        }
    },
    "name": "mcustiel/codeception-wiremock-extension",
    "type": "library",
    "description": "Wiremock extension for Codeception",
    "license": "GPL-3.0+",
    "require": {
        "php": ">=8.0",
        "codeception/codeception": "^5.0.0",
        "wiremock-php/wiremock-php": "^2.32.0"
    },
    "require-dev": {
        "codeception/codeception": "^5.0.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "phing/phing": "^2.17.4",
        "squizlabs/php_codesniffer": "^3.7.1",
        "phpmd/phpmd": "^2.13.0",
        "sebastian/phpcpd": "^6.0.3",
        "pdepend/pdepend": "^2.12.1",
        "phploc/phploc": "^7.0.0",
        "phpdocumentor/phpdocumentor": "^v3.0.0",
        "codeception/module-phpbrowser": "*",
        "codeception/module-asserts": "*"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}